{
    "manifest_version": 2,
    "name": "Order Tabs by Domain 2.0",
    "version": "2.0.0",
    "description": "Tabs ordered by domain, as the Flying Spaghetti Monster intended. Ramen.",
    "homepage_url": "https://github.com/jpfleischer/w3x-taborder",
    "icons": {
        "16": "icons/tab-16.png",
        "32": "icons/tab-32.png",
        "48": "icons/tab-48.png",
        "64": "icons/tab-64.png",
        "96": "icons/tab-96.png"
    },
    "permissions": ["tabs", "bookmarks", "storage", "menus", "contextMenus"],

    "background": {
        "scripts": ["main.js"]
    },

    "options_ui": {
        "page": "prefs.xhtml"
    },

    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "16": "icons/tab-16.png",
            "32": "icons/tab-32.png",
            "48": "icons/tab-48.png",
            "64": "icons/tab-64.png",
            "96": "icons/tab-96.png"
        },
        "default_title": "Order Tabs by Domain 2.0"
    }
}
